{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2020",
    "jsx": "react-jsx",
   "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "lib": ["esnext"],
    "types": ["react", "react-native"]
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
