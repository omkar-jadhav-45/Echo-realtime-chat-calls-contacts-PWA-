# Echo Auth Service .env example
# Copy to .env and set strong secrets in production

# Either set single secret:
JWT_SECRET=change-me
# Or set multiple secrets for rotation (kid1:secret1,kid2:secret2,...)
# JWT_SECRETS=kid1:change-me-1,kid2:change-me-2
# Optionally set the active KID used to issue tokens:
# JWT_ACTIVE_KID=kid2
PORT=8080
RUST_LOG=info
